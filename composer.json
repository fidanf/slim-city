        "symfony/var-dumper": "^3.2",
        "vlucas/phpdotenv": "^2.4",
        "nesbot/carbon": "1.22.1",
        "swiftmailer/swiftmailer": "5.4",
        "illuminate/database": "^5.4",
        "league/fractal": "0.16.0",
        "monolog/monolog": "1.23.0",
        "predis/predis": "v1.1.1"
    },
    "autoload": {
